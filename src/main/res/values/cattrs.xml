        <attr name="cmarginBottom" />
        <attr name="cmarginBottomByWidth" />
        <attr name="cmarginByWidth" />
        <attr name="cmarginByHeight" />
        <attr name="cpaddingLeft" />
        <attr name="cpaddingRight" />
        <attr name="cpaddingTop" />
        <attr name="cpaddingTopByWidth" />
        <attr name="cpaddingBottom" />
        <attr name="cpaddingBottomByWidth" />
        <attr name="cminWidth" />
        <attr name="cmaxWidth" />
        <attr name="cmaxWidthPx" />
        <attr name="cminHeight" />
        <attr name="cmaxHeight" />
        <attr name="ctoSquare" />
